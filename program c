#include<stdio.h>

int main()
{
int num1;
int num2;
int num3;
int result

printf("input the first number: ");
scanf("%d" , &num1);
printf("input the second number: ");
scanf("%d" , &num2);
printf("input the third number: ");
scanf("%d" , &num3);
//divided and times
result = num1 / num2 * num3
printf("\n%d divided %d * %d" , num1, num2, num3, result);
return 0;
}
